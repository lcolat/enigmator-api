{
  "name": "Enigme",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "number",
      "required": true
    },
    "question": {
      "type": "string"
    },
    "reponse": {
      "type": "string",
      "required": true
    },
    "etats": {
      "type": "boolean",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "Enigme_User": {
      "type": "belongsTo",
      "model": "UserEnigmator",
      "foreignKey": "UserID",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "media": {
      "type": "belongsTo",
      "model": "Media",
      "foreignKey": "MediaID",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
